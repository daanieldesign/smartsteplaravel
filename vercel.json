{
    "version": 2,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.4.0"
        }
    },
    "routes": [
        {
            "src": "/(css|js)/(.*)$",
            "dest": "/api/assets.php?type=$1&file=$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
